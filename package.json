    "@types/react": "^19.0.0",
    "@types/react-collapse": "^5.0.4",
    "@types/react-dom": "^19.0.0",
    "prisma": "^6.5.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
